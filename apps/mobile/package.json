{
  "version": "0.1.0",
  "name": "@pegada/mobile",
  "main": "index.js",
  "private": true,
  "installConfig": {
    "hoistingLimits": "workspaces"
  },
  "workspaces": {
    "nohoist": [
      "@bugsnag/plugin-expo-eas-sourcemaps",
      "@bugsnag/source-maps"
    ]
  },
  "scripts": {
    "ampli:setup": "ampli pull",
    "start": "EXPO_USE_METRO_WORKSPACE_ROOT=1 expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "eject": "expo eject",
    "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "pnpm lint --fix",
    "format:fix": "prettier --write . --ignore-path ../../.gitignore",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "expo:prebuild": "expo prebuild",
    "setup:secret:files": "[ ! -s ./google-services.json ] && echo $GOOGLE_SERVICES_JSON > ./google-services.json || echo \"\" ; [ ! -s ./GoogleService-Info.plist ] && echo $GOOGLE_SERVICE_INFO_PLIST > ./GoogleService-Info.plist || echo \"\"",
    "preinstall": "pnpm setup:secret:files",
    "eas-build-on-success": "npx bugsnag-eas-build-on-success"
  },
  "dependencies": {
    "@amplitude/ampli": "^1.35.0",
    "@amplitude/analytics-react-native": "^1.4.9",
