  "dependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
    "prettier": "^3.3.3"
  },
  "devDependencies": {
    "@pegada/tsconfig": "workspace:*",
    "typescript": "*"
  },
  "prettier": "@pegada/prettier-config"
}
